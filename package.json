{
  "name": "xast-util-from-xml",
  "version": "2.0.1",
  "description": "xast utility to parse from XML",
  "license": "MIT",
  "keywords": [
    "unist",
    "xast",
    "xast-util",
    "util",
    "utility",
    "xml",
    "extensible",
    "markup",
    "language",
    "parse",
    "syntax",
    "tree",
    "ast"
  ],
  "repository": "syntax-tree/xast-util-from-xml",
  "bugs": "https://github.com/syntax-tree/xast-util-from-xml/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/sax": "^1.0.0",
    "@types/unist": "^2.0.0",
    "@types/xast": "^1.0.0",
    "sax": "^1.0.0",
    "vfile-message": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "c8": "^7.0.0",
    "is-hidden": "^2.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^11.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "tape": "^5.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "xo": "^0.53.0"
  },
  "scripts": {
    "prepack": "npm run build && npm run format",
    "build": "tsc --build --clean && tsc --build && type-coverage",
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node --conditions development test/index.js",
    "test-coverage": "c8 --check-coverage --100 --reporter lcov npm run test-api",
    "test": "npm run build && npm run format && npm run test-coverage"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "rules": {
      "unicorn/prefer-code-point": "off"
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "rules": {
          "no-await-in-loop": 0
        }
      }
    ]
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": true
  }
}
