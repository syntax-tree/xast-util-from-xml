{
  "name": "xast-util-from-xml",
  "version": "1.0.1",
  "description": "xast utility to parse from XML",
  "license": "MIT",
  "keywords": [
    "unist",
    "xast",
    "xast-util",
    "util",
    "utility",
    "xml",
    "extensible",
    "markup",
    "language",
    "parse",
    "syntax",
    "tree",
    "ast"
  ],
  "repository": "syntax-tree/xast-util-from-xml",
  "bugs": "https://github.com/syntax-tree/xast-util-from-xml/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "sax": "^1.0.0",
    "vfile-message": "^2.0.0"
  },
  "devDependencies": {
    "is-hidden": "^1.0.0",
    "negate": "^1.0.0",
    "nyc": "^15.0.0",
    "prettier": "^2.0.0",
    "remark-cli": "^9.0.0",
    "remark-preset-wooorm": "^8.0.0",
    "tape": "^5.0.0",
    "xo": "^0.38.0"
  },
  "scripts": {
    "format": "remark . -qfo && prettier . -w --loglevel warn && xo --fix",
    "test-api": "node test",
    "test-coverage": "nyc --reporter lcov tape test/index.js",
    "test": "npm run format && npm run test-coverage"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "functions": 100,
    "branches": 100
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false,
    "rules": {
      "complexity": "off",
      "no-misleading-character-class": "off",
      "unicorn/escape-case": "off",
      "unicorn/no-hex-escape": "off",
      "unicorn/no-fn-reference-in-iterator": "off",
      "unicorn/prefer-optional-catch-binding": "off"
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
